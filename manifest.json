{
  "name": "Gumtree Checker",
  "version": "1.0",
  "description": "Checks Gumtree",
  "icons": {
	"16": "images/icon_16.png",
	"48": "images/icon_48.png",
	"128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": {                    
      "19": "images/icon_19.png",         
      "38": "images/icon_38.png"          
    },
    "default_title": "Gumtree Checker"
  },
  "permissions": [
        "tabs",
		"notifications",
		"http://*/*"
   ],
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
